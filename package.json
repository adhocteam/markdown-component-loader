{
  "name": "markdown-component-loader",
  "version": "1.1.0",
  "description": "Turn Markdown into dynamic, stateless React components",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js,**/__fixtures__,**/__mocks__\"",
    "test": "jest",
    "build": "yarn prepublish && webpack",
    "start": "webpack-dev-server",
    "lint": "eslint .",
    "lint-and-fixup": "eslint --fix ."
  },
  "keywords": [
    "markdown",
    "markdown-component",
    "markdown-it",
    "component",
    "webpack",
    "react",
    "jsx"
  ],
  "homepage": "https://github.com/ticky/markdown-component-loader",
  "bugs": {
    "url": "https://github.com/ticky/markdown-component-loader/issues"
  },
  "repository": "https://github.com/ticky/markdown-component-loader.git",
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "dependencies": {
    "any-base": "^1.0.0",
    "doc-chomp": "1.1.0",
    "front-matter": "^2.1.0",
    "hastml": "^0.1.0",
    "he": "^1.1.1",
    "highlight.js": "^9.6.0",
    "jsesc": "^2.2.0",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.1.0",
    "sha.js": "^2.4.5"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-version-inline": "1.0.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "codemirror": "5.39.2",
    "css-loader": "0.28.9",
    "eslint": "4.19.1",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "image-webpack-loader": "3.6.0",
    "jest": "21.2.1",
    "json-loader": "0.5.7",
    "markdown-it-anchor": "4.0.0",
    "markdown-it-table-of-contents": "0.4.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-codemirror2": "2.0.2",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.3.2 || ^16.0"
  },
  "babel": {
    "plugins": [
      "version-inline",
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ]
  }
}
